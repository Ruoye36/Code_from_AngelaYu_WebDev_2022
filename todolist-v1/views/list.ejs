<%- include("header") %>
  <div class="box" id="heading">
    <h1><%= listTitle%></h1>
  </div>

<div class="box">
  <div class="item">
    <input type="checkbox">
    <p>Example todo 1</p>
  </div>
    <%  for(var i=0;i<newListItems.length;i++){ %>
    <!--or: var newItem of newListItems-->
    <div class="item">
      <input type="checkbox">
      <p><%=newListItems[i]%></p>
    </div>
    <%}%>
    <form class="item" action="/" method="post">
      <input type="text" name="nextTodo" placeholder="What next?" autocomplete="off">
      <button type="submit" name="listAddBtn" value=<%=listTitle%>>+</button>
    </form>
</div>
<%- include("footer") %>
